<!DOCTYPE html>
<html lang="en">
  <%- include("../partials/head") %>

  <body>
    <%- include("../partials/header") %>

    <section class="main_contatiner_usersList">
     
        <h2>Listado de Usuarios</h2>
        <!-- usuarios -->
          <!-- **************************************** -->
          <% usersList .forEach(usuario =>{%>
                <div class="container_usersList">
                    <div class="container_img">
                        <img  src="<%=usuario.image%>" />
                    </div>
                    <div class="container_dates">

                        <h3 id="name">Nombre: <%= usuario.first_name %> <%= usuario.last_name %></h3>
                    
                        <h3 id="email">Email: <%= usuario.email %></h3>
                        <h3 id="type">Tipo: <%= usuario.tipo %></h3> 
                    </div>
                </div>
            <% }) %>
          <!-- **************************************** -->
    
     
    </section>
    <%- include("../partials/footer") %>
  </body>
</html>